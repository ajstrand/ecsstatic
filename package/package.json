{
	"name": "@acab/ecsstatic",
	"description": "The predefinite CSS-in-JS library for Vite.",
	"version": "0.7.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mayank99/ecsstatic.git"
	},
	"author": "Mayank",
	"keywords": [
		"css-in-js",
		"vite",
		"static"
	],
	"type": "module",
	"main": "index.cjs",
	"module": "index.js",
	"types": "index.d.ts",
	"files": [
		"*.js",
		"*.cjs",
		"*.d.ts",
		"!overrides.d.ts"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./scss": {
			"types": "./scss.d.ts",
			"import": "./scss.js",
			"require": "./scss.cjs"
		},
		"./vite": {
			"types": "./vite.d.ts",
			"import": "./vite.js",
			"require": "./vite.cjs"
		}
	},
	"dependencies": {
		"acorn-walk": "^8.3.2",
		"autoprefixer": "^10.4.17",
		"esbuild": "^0.20.0",
		"esbuild-plugin-noexternal": "^0.1.6",
		"magic-string": "^0.30.6",
		"postcss": "^8.4.33",
		"postcss-nested": "^6.0.1",
		"postcss-nesting": "^12.0.2",
		"postcss-scss": "^4.0.9"
	},
	"devDependencies": {
		"@types/estree": "^1.0.5",
		"@types/node": "^20.11.16",
		"tsup": "^8.0.1",
		"vite": "^5.0.12"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"sideEffects": false
}
